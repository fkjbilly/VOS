@model VOS.ViewModel.BasicData.CategoryVMs.CategoryVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:textbox field="Entity.Name" />
        <wt:combobox field="Entity.ParentId" items="AllParents" />
        <wt:textbox field="Entity.CycleNum" />
        <wt:textbox field="Entity.Cycle" />
    </wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
